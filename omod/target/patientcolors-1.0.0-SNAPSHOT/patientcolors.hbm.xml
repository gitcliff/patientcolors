<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    <hibernate-mapping package="org.openmrs.module.patientcolors">
    <class name="patientcolors" table="patient_colors">
        <id name="patientcolorId" type="int" column="patientcolor_id" unsaved-value="0">
            <generator class="native" />
        </id>
        <discriminator column="patientcolor_id" insert="false" />
        <property name="patientcolor_Id" type="int" column="patientcolor_id" length="38" unique="true" />
        <property name="name" type="java.lang.String" column="name" length="255" unique="true" /> 
    </class>
</hibernate-mapping>